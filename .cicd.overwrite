---

# Generic
generic:
  role: ansible-role-gpg
  role_short: gpg
  role_description: |
    Installs gpg and manage keys
  collections: []

# Dependencies
dependencies:
  roles:
    - deitkrachten.facts
  collections: []

# Molecule
molecule:
  requirements:
    roles_extra: []
  converge:
    vars:
      gpg_user_name: root
      gpg_user_realname: root
      gpg_user_comment: "test key"
      gpg_user_email: "root@localhost"
      gpg_user_passphrase: "Abcd1234"
      # gpg_user_key: "test"
